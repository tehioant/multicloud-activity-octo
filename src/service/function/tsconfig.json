{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "outDir": "dist",
    "rootDir": ".",
    "sourceMap": true,
    "strict": false,
    "typeRoots": [ "node_modules/@types" ],
    "noImplicitReturns": true
  },
  "compileOnSave": true,
  "skipLibCheck": true,
  "exclude": [ "node_modules", "functions" ],
  "include": [ "**/*.ts", "**/*.tsx" ]
}